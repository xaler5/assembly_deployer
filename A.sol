pragma solidity <0.6.0;

import "./Deployer.sol";

contract A {

    bytes bytecodeB = '';

    address deployerAddress;

    Deployer deployer;

    constructor (address adr) public {
        deployer = Deployer(adr);
    }

    function deployFromBytecode() public returns(address) {

        address tmp = deployer.deployCode(bytecodeB);

        return tmp;
    }

}
